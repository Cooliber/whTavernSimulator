# Production environment variables for Vercel deployment
# These should be set in Vercel dashboard

# API Configuration
GROQ_API_KEY=
CEREBRAS_API_KEY=
OPENAI_API_KEY=

# Model Configuration
GROQ_MODEL=llama-3.1-70b-versatile
CEREBRAS_MODEL=llama3.1-8b
OPENAI_MODEL=gpt-4

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO
NODE_ENV=production

# Performance Settings
MAX_AGENTS=17
CACHE_ENABLED=true
RETRY_ATTEMPTS=3

# CORS Configuration
ALLOWED_ORIGINS=*

# Database Configuration
DATABASE_URL=sqlite:///tmp/tavern.db

# UI Configuration
GSAP_PERFORMANCE_MODE=high